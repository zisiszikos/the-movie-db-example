{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jest/recommended"
    ],
    "plugins": [
        "react",
        "jest"
    ],
    "rules": {
        "react/prop-types": 0
    },
    "globals": {
        "Promise": true,
        "shallow": true,
        "mount": true,
        "render": true,
        "delayTest": true
    },
    "env": {
        "browser": true,
        "node": true,
        "jest/globals": true
    }
}
