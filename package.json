{
  "name": "the-movie-db-example",
  "version": "1.2.0",
  "description": "The Movie DB example project.",
  "scripts": {
    "start": "node app/server.js",
    "start:dev": "grunt",
    "build": "webpack -p --config webpack.config.prod.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "watch": "webpack --watch --config webpack.config.dev.js",
    "analyze-build": "webpack --config webpack.config.analyzer.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test-cy": "node app/server.js & wait-on http://localhost:3000 && cypress run && kill -9 $!"
  },
  "jest": {
    "setupFiles": [
      "./jestshim.js",
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "notify": true,
    "roots": [
      "<rootDir>/app/src/"
    ],
    "testMatch": [
      "**/*.test.js?(x)",
      "**/__tests__/**/*.js?(x)"
    ],
    "collectCoverageFrom": [
      "app/src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/app/src/index.react.js",
      "<rootDir>/app/src/general/helpers.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/src",
      "app"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zisiszikos/the-movie-db-example.git"
  },
  "author": "Zisis Zikos",
  "license": "MIT",
  "dependencies": {
    "axios": "0.21.2",
    "babel-polyfill": "6.26.0",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "connect-redis": "3.3.2",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "http-errors": "1.6.2",
    "lusca": "1.5.2",
    "morgan": "1.9.1",
    "pubsub-js": "1.5.7",
    "pug": "3.0.1",
    "serve-favicon": "2.4.5"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.17",
    "css-loader": "0.28.7",
    "cypress": "8.6.0",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "enzyme-to-json": "3.1.4",
    "eslint": "4.10.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-jest": "21.2.0",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "grunt": "1.3.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-develop": "0.4.0",
    "jest": "21.2.1",
    "nock": "9.0.27",
    "node-sass": "7.0.0",
    "react": "16.0.0",
    "react-dom": "16.0.1",
    "react-test-renderer": "16.0.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "wait-on": "2.0.2",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "3.3.2"
  },
  "engines": {
    "node": "12"
  }
}
